{
  "name": "todo-workspace-nestjs-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["todo-workspace-nestjs"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/todo-workspace-nestjs-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": [
        "todo-workspace-nestjs:build",
        "todo-workspace-nestjs:serve"
      ]
    }
  }
}
